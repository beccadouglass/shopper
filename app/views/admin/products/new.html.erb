<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add A Product</h1>
    <%= simple_form_for @product, url: product_admin_products_path do |f| %>
    <%= f.input :title %>
    <%= f.input :image_filename %>
    <%= f.input :description %>
    <%= f.input :price %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>